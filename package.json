{
  "name": "expense",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate-models": "prisma generate",
    "update-rates": "ts-node --esm scripts/update-rates.mts",
    "compile": "tsc --noEmit",
    "lint": "next lint",
    "prettier": "prettier --write '**/*.(js|ts|mts|tsx|md|json)'",
    "prettier:ci": "prettier --check '**/*.(js|ts|mts|tsx|md|json)'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.11.0",
    "@svgr/webpack": "^6.5.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "dotenv-flow": "^3.2.0",
    "inter-ui": "^3.19.3",
    "next": "^13.2.3",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.20.1",
    "node-fetch": "^3.3.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.0",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "@typescript-eslint/no-empty-function": "off"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "**/*.(js|ts|mts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": "prettier --write"
  }
}
