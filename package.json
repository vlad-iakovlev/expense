{
  "name": "expense",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "npm run migrations:apply-all && prisma db push && npm run start",
    "migrations": "ts-node --esm -r dotenv/config bin/migrations.ts",
    "migrations:create": "npm run migrations -- --create",
    "migrations:apply-all": "npm run migrations -- --applyAll",
    "update-currency-names": "ts-node --esm -r dotenv/config bin/update-currency-names.ts",
    "update-currency-rates": "ts-node --esm -r dotenv/config bin/update-currency-rates.ts",
    "generate-icons": "ts-node --esm bin/generate-icons.ts",
    "generate-splash": "ts-node --esm bin/generate-splash.ts",
    "compile": "tsc",
    "lint": "next lint",
    "lint:ci": "DEBUG=eslint:cli-engine next lint",
    "prettier": "prettier --write '**/*.(js|ts|tsx|md|json)'",
    "prettier:ci": "prettier --check '**/*.(js|ts|tsx|md|json)'",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --verbose --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.2.0",
    "date-fns": "^2.30.0",
    "default-import": "^1.1.5",
    "dotenv": "^16.0.3",
    "framer-motion": "^10.12.16",
    "immer": "^10.0.2",
    "mongodb": "^5.5.0",
    "next": "^13.4.4",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.22.1",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.1",
    "node-html-parser": "^6.1.5",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "use-debounce": "^9.0.4",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "storybook": "^7.0.18",
    "typescript": "^5.0.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:@typescript-eslint/strict",
      "prettier",
      "plugin:storybook/recommended"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "importOrder": [
      "^\\.\\./",
      "^\\./"
    ],
    "importOrderSortSpecifiers": true
  },
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": "prettier --write"
  }
}
